   },

   "source": [

    "# Parameters\n",

    "GROUP_ID = '43'\n",


    "ALGORITHM = 'ga' # bt | fc | ac3 | sa | ga\n",


    "ALGORITHM = 'ac3' # bt | fc | ac3 | sa | ga\n",

    "PUZZLE_TYPE = 'extreme' # easy | medium | hard | extreme\n",


    "PUZZLE_PATH = 'puzzles/extreme/Evil-P4.txt'"


    "PUZZLE_PATH = f\"puzzles/{PUZZLE_TYPE}/Evil-P2.txt\""

   ],

   "outputs": [],

   "execution_count": null

  },

  {

   "metadata": {},

   "cell_type": "code",

   "source": [

    "from src.io import read_puzzle, write_puzzle, build_output_filename\n",

    "from src.solve import solve\n",

    "import hashlib\n",

    "\n",

    "# Deterministic seed from parameters\n",

    "_seed = int(hashlib.md5(f\"{GROUP_ID}:{ALGORITHM}:{PUZZLE_PATH}\".encode()).hexdigest()[:8], 16)"

   ],

   "id": "ec2305eddd05134",

   "outputs": [],

   "execution_count": null

  },

  {

   "metadata": {},

   "cell_type": "code",

   "source": [

    "grid = read_puzzle(PUZZLE_PATH)\n",

    "solved, metrics = solve(grid, ALGORITHM, seed=_seed)\n",

    "\n",

    "outfile = build_output_filename(GROUP_ID, ALGORITHM, PUZZLE_TYPE, PUZZLE_PATH)\n",

    "if solved is None:\n",

    "    raise RuntimeError(f\"No solution found for {PUZZLE_PATH} using {ALGORITHM}. Metrics: {metrics}\")\n",

    "\n",

    "write_puzzle(solved, outfile)\n",

    "print(\"Saved:\", outfile)\n",

    "print(\"Metrics:\", metrics)"

   ],

   "id": "6cc3f883c5710aca",

